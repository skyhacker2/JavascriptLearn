<%if(totalItems) {%>
	<div class="campaign-list">
		<%for(var i = 0, l = campaigns.length; i < l; i++) {
			var campaign = campaigns[i];%>
			<div class="campaign <%=campaign.status%>">
				<div class="flag"><i></i><%=lang.campaign.status[campaign.status]%></div>
				<div class="tags">
					<i class="icon-tags"></i> Archive
				</div>
				<div class="op">
					<%if(campaign.status == 'DRAFT' || campaign.status == 'PENDING' || campaign.status == 'APPROVED') {%>
						<a href="html/campaign/edit-<%=G.LANG%>.html#!edit/<%=base64Params%>/campaign/<%=campaign.id%>"><i class="icon-pencil"></i> Edit</a> 
					<%}%>
					<a href="html/campaign/edit-<%=G.LANG%>.html#!duplicate/<%=base64Params%>/campaign/<%=campaign.id%>"><i class="icon-copy"></i> Duplicate</a> 
					<%if(campaign.status == 'PENDING' || campaign.status == 'APPROVED' || campaign.status == 'EXECUTING') {%>
						<a href="javascript:void(0);" onclick="return false;" data-rfl-click="abortCampaign <%=i%>"><i class="icon-minus-sign"></i> Abort</a> 
					<%} else {%>
						<a href="javascript:void(0);" onclick="return false;" data-rfl-click="delCampaign <%=i%>"><i class="icon-remove"></i> Delete</a> 
					<%}%>
				</div>
				<div class="head">
					<div class="main">
						<div class="img">
							<a href="javascript:void(0);" data-rfl-click="previewHtmlContent <%=i%>"><img src="http://snapshot.manggis.internal/snapshot/campaign/<%=campaign.id%>.png" /></a>
						</div>
						<div class="info">
							<h3><a href="html/campaign/edit-<%=G.LANG%>.html#!view/<%=base64Params%>/overview/<%=campaign.id%>/list" title="<%==campaign.name%>"><%==campaign.name%></a></h3>
							<div>
								Regular Campaign
							</div>
							<div>
								Created at <%=$opt.util.formatDateTime(campaign.createTime, 'yyyy-MM-dd HH:mm')%>
							</div>
						</div>
					</div>
					<div class="stat">
						<p class="rate"><%=campaign.openRate || '0.00'%>%</p>
						<p>OPEN RATE</p>
					</div>
					<div class="stat">
						<p class="rate"><%=campaign.clickRate || '0.00'%>%</p>
						<p>CLICK RATE</p>
					</div>
					<div class="stat">
						<p class="rate"><%=campaign.bounceRate || '0.00'%>%</p>
						<p>BOUNCE RATE</p>
					</div>
				</div>
			</div>
		<%}%>
	</div>
	<div class="pagination-right section clearfix">
		<!-- include "../../../html/inc/ajax-history-pager.tpl.html" -->
	</div>
<%} else {%>
	<div class="guiding">
		<div class="icon-part">
			<i class="icon-frown"></i>
		</div>
		<div class="text-part">
			<h1>Can not find the campaign</h1>
			<p class="desc">
				Sorry, we can not find any campaign matching your request.
			</p>
			<button class="btn btn-primary btn-lg" data-rfl-click="createCampaign">Create Campaign <i class="icon-circle-arrow-right"></i></button>
		</div>
	</div>
<%}%>