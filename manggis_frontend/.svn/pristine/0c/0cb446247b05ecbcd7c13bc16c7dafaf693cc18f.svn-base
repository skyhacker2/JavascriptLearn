<!DOCTYPE html>
<html lang="%{{_lang_}}%">
<head>
<meta charset="utf-8" />
<meta http-equiv="content-type" content="text/html; charset=utf-8" />
<title>${{campaign.label.manageCampaigns}}$ - ${{common.appName}}$</title>
<!-- include "../inc/head.inc.html" -->
<script type="text/javascript">
document.write('<link rel="stylesheet" href="css/mod/campaign/list-main.css" />');
</script>
</head>
<body>
<!-- include "../inc/body.inc.html" -->

<!-- require "../../js/lang/%{{_lang_}}%/campaign" -->
<!-- require "../../js/mod/campaign/list" -->
<script type="text/javascript">
G.pageRequire(['../../js/mod/campaign/list', '../../js/mod/nav-bar/init', 'ajax!' + G.getAjaxLoadUrl('users/information')], function(main, navBar, auth) {
	require(['rfl'], function(rfl) {
		rfl.auth.init(auth, function() {
			var params = G.getAjaxHistoryParams();
			navBar.init('list', {searchType: 'campaign', listId: params[0]});
			main.init();
		});
	});
}, function() {
	require(['rfl'], function(rfl) {
		rfl.ui.renderPageLoadError('#main-div');
	});
});
</script>
</body>
</html>