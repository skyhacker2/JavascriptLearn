<h3 class="text-center">You have choosen to import customers for delivery</h3>
<%if(data.status == 'completed') {%>
	<div class="alert alert-success section-sm text-center">
		The importing job has been completed!
	</div>
<%} else if(data.status == 'inprogress') {%>
	<div class="alert alert-info section-sm text-center">
		The importing job is in progress. 
	</div>
<%} else {%>
	<div class="alert alert-danger section-sm text-center">
		The importing job has been terminated! 
	</div>
<%}
var stat = [];
%>
<%if(data.info.added) {
	stat.push(data.info.added + ' customers added');
}%>
<%if(data.info.updated) {
	stat.push(data.info.updated + ' customers updated');
}%>
<%if(data.info.notAdded) {
	stat.push(data.info.notAdded + ' customers not added');
}%>
<%if(stat.length) {%>
	<div class="text-center">
		<%=stat.join(', ')%><br />
	</div>
<%}%>
<div class="text-center section-sm">
<a class="btn btn-default btn-sm" href="html/customer/import-<%=G.LANG%>.html#!<%=listId%>/finished/<%=data.id%>">View the job detail</a>
</div>