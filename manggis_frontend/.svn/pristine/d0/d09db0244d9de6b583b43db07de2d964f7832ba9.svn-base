<%for(var i = blocks.length; i--;){%>
	<div align="center" style="float:left;width:100px;">
	<a tabindex="-1" href="javascript:void(0)" onclick="return false;" data-template="<%=blocks[i].type%>"><img src="<%=blocks[i].ico%>" data-rfl-click="changeBlock <%=blocks[i].type%>"/></a>
	<p><%=blocks[i].description%></p>
	</div>
<%}%>