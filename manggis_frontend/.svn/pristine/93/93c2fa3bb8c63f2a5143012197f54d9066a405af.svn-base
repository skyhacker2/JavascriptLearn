<%if(issueJobs.length) {%>
	<h3><%=issueJobs.length%> job issue(s)</h3>
	<%
	var i, l, job;
	for(var i = 0, l = issueJobs.length; i < l; i++) {
		job = issueJobs[i];
	%>
		<ul>
			<li><i class="icon-warning-sign"></i> <%=job.info.notAdded%> customer wasn't added. <a href="html/customer/import-<%=G.LANG%>.html#!<%=listId%>/<%=base64Info%>/finished/<%=job.id%>/overview">See why</a> <i class="icon-remove-sign"></i></li>
			<%
			job = issueJobs[++i];
			if(job) {
			%>
				<li><i class="icon-warning-sign"></i> <%=job.info.notAdded%> customer wasn't added. <a href="html/customer/import-<%=G.LANG%>.html#!<%=listId%>/<%=base64Info%>/finished/<%=job.id%>/overview">See why</a> <i class="icon-remove-sign"></i></li>
			<%}%>
		</ul>
	<%}%>
	<%if(totalPages > 1) {%>
		<div class="text-right">
			<%if(page > 1) {%>
				<a href="javascript:void(0);" onclick="return false;" data-rfl-click="renderIssueJobs <%=page - 1%>"><i class="icon-chevron-left"></i></a> 
			<%} else {%>
				<i class="icon-chevron-left"></i> 
			<%}%>
			&nbsp; 
			<%if(page < totalPages) {%>
				<a href="javascript:void(0);" onclick="return false;" data-rfl-click="renderIssueJobs <%=page + 1%>"><i class="icon-chevron-right"></i></a>
			<%} else {%>
				<i class="icon-chevron-right"></i> 
			<%}%>
		</div>
	<%}%>
<%} else {%>
	<div class="no-jobs">
		<i class="icon-warning-sign"></i> Job issues will go here if any.
	</div>
<%}%>
		