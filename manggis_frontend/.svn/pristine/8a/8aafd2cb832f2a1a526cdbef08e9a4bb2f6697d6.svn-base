<!DOCTYPE html>
<html lang="%{{_lang_}}%">
<head>
<meta charset="utf-8" />
<meta http-equiv="content-type" content="text/html; charset=utf-8" />
<title>${{user.label.manageUsers}}$ - ${{common.appName}}$</title>
<!-- include "../inc/head.inc.html" -->
</head>
<body>
<!-- include "../inc/body.inc.html" -->

<!-- require "../../js/lang/%{{_lang_}}%/user" -->
<!-- require "../../js/mod/user/controllers/user-list" -->
<script type="text/javascript">
G.pageRequire(['../../js/mod/user/controllers/user-list', '../../js/mod/nav-bar/init', 'ajax!' + G.getAjaxLoadUrl('users/information')], function(UserList, navBar, auth) {
	require(['rfl'], function(rfl) {
		rfl.auth.init(auth, function() {
			navBar.init('user');
			UserList.init({el: '#main-div'});
		});
	});
}, function() {
	require(['rfl'], function(rfl) {
		rfl.ui.renderPageLoadError('#main-div');
	});
});
</script>
</body>
</html>