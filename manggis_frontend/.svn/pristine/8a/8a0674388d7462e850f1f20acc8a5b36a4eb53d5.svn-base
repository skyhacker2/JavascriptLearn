<%
var rfl = require('rfl');
var langResourceCommon = require('lang/{{G.LANG}}/common');
var langResourceCustomer = require('lang/{{G.LANG}}/customer');
var langResourceCustomerImport = require('lang/{{G.LANG}}/customer-import');
%>

<p class="desc">
	The job progress is <strong><%=data.progress%>%</strong>. 
	<%if(leftTime > 0) {%>
		It still needs <strong><%=rfl.util.formatLeftTime(leftTime, ' hours ', ' minutes ', ' seconds ')%></strong> to finish this job.
	<%} else {%>
		It still needs <strong>over 10 minutes</strong> to finish this job.
	<%}%>
</p>